#include "../common/STD_PKT_COMMON.def"

//
// SCTP CHUNK_ASCONF
//
PKG_SCTP_SHORT_SEND_AUTH(
	sctp_chunk_asconf_unknown_param,
	{
		chunk = chunk_asconf_unknown_param;
	}
)

CHUNK_ASCONF chunk_asconf_unknown_param {
	SerialNumber = SSERIAL;
	Param = addr;
	Param = addr;
}

IPv4Address addr {
	Address = v4(SCTP4_TN_NET0_ADDR);
}

//
// SCTP CHUNK_ASCONF_ACK
//
PKG_SCTP_SHORT_RECV_AUTH(
	sctp_chunk_asconf_ack_err,
	{
		chunk = chunk_asconf_ack_err;
	}
)

CHUNK_ASCONF_ACK chunk_asconf_ack_err {
	SerialNumber = SSERIAL;
	Param = errindication;
}

ErrorCauseIndication errindication {
	Error = err_unknown_param;
}

UnrecognizedParameters err_unknown_param {
	Parameters = addr;
}

