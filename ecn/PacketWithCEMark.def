#include "../common/STD_PKT_COMMON.def"

//
// SCTP CHUNK_DATA
//
PKG_SCTP_NORM_SEND(
	sctp_chunk_data_mark_ce,
	{
		_SRC(SCTP_TN_NET0_ADDR);
		_DST(SCTP_NUT_NET0_ADDR);
		_ECN(3);
	},
	{
		chunk = chunk_data_snd;
	}
)

//
// SCTP CHUNK_SACK and CWR
//
PKG_SCTP_SHORT_RECV(
	sctp_chunk_ecne_and_sack,
	{
		chunk = chunk_ecn_echo_rcv;
		chunk = chunk_sack_rcv;
	}
)

//
// SCTP CHUNK_ECN_ECHO
//
PKG_SCTP_SHORT_RECV(
	sctp_chunk_ecn_echo_rcv,
	{
		chunk = chunk_ecn_echo_rcv;
	}
)

CHUNK_ECN_ECHO chunk_ecn_echo_rcv {
#ifndef SNDTSN
	LowestTSNNumber = INITTSN;
#else
	LowestTSNNumber = SNDTSN;
#endif
}


//
// SCTP CHUNK_ECN_CWR
//
PKG_SCTP_SHORT_SEND(
	sctp_chunk_ecn_cwr_snd,
	{
		chunk = chunk_ecn_cwr_snd;
	}
)

CHUNK_ECN_CWR chunk_ecn_cwr_snd {
#ifndef SNDTSN
	LowestTSNNumber = INITTSN;
#else
	LowestTSNNumber = SNDTSN;
#endif
}

