#include "../common/STD_PKT_COMMON.def"

#undef VERFTAG
#define VERFTAG	INITTAG

//
// SCTP CHUNK_HEARTBEAT_ACK
//
PKG_SCTP_SHORT_SEND(
	sctp_chunk_heartbeat_ack_ootb,
	{
		chunk = chunk_heartbeat_ack_ootb;
	}
)

CHUNK_HEARTBEAT_ACK chunk_heartbeat_ack_ootb {
	Information = heartbeat_info;
}

HeartbeatInfo heartbeat_info {
	Data = repeat(0x00, 16);
}

//
// SCTP CHUNK_ABORT
//
PKG_SCTP_SHORT_RECV(
	sctp_chunk_abort_ootb,
	{
		chunk = chunk_abort_ootb;
	}
)

CHUNK_ABORT chunk_abort_ootb {
	TFlag = 1;
	Length = stop;
}

