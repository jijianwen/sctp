#include "../common/STD_PKT_COMMON.def"

#if ENABLE_IPV6
#define SCTP_TN_NET0_MCAST_ADDR	_IPV6_SOLNODE_MCAST(tnv6())
#else
#define SCTP_TN_NET0_MCAST_ADDR	v4("224.0.0.1")
#endif

//
// SCTP CHUNK_DATA
//
PKG_SCTP_NORM_SEND(
	sctp_chunk_data_src_nonunicast,
	{
		_SRC(SCTP_TN_NET0_MCAST_ADDR);
		_DST(SCTP_NUT_NET0_ADDR);
	} ,
	{
		chunk = chunk_data_snd;
	}
)

